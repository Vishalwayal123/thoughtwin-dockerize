<body>
  <% if params[:controller] == "home" && params[:action] == "android" || params[:action] == "nodejs" || params[:action] == "python" || params[:action] == "ror" || params[:action] == "reactdev" || params[:action] == "angular" || params[:action] == "phpdev" || params[:action] == "mernstack" || params[:action] == "meanstack" || params[:action] == "ios" || params[:action] == "reactnative" %>
  <%= render 'layouts/inner_pages_header' %>
  <% elsif params[:controller] == "admins" && params[:action] == "index"||params[:controller] == "admins" && params[:action] == "show"||params[:controller] == "devise/registrations" && params[:action] == "edit"%>
  <% else %>
  <%= render 'layouts/header' %>

  <% end %>
  <!--   <p class="notice"><%= notice %></p>
    <p class="alert"><%= alert %></p>  -->
    <%= render 'toastr_rails/flash' %>
    <%= yield %>
    <!-- ChatCode  -->
    <div>
      <button class="chatbox-open">
        <img src="/assets/img/mess.png">
      </button>
      
      <section class="chatbox-popup">
       
        <div class="chatbox-popup__div">
          <aside>
            <i class="fa fa-user-circle fa-4x chatbox-popup__avatar" aria-hidden="true"></i>
          </aside>
          <aside>
            <h1>TWbot</h1>
          </aside>

           <button class="chatbox-close">
        <img src="/assets/img/close.png">
      </button>
        </div> 
          <div id='bodybox'>
          <div id='chatborder'>
            <p class="chat_time"> <%= Time.now.strftime(" Today %I:%M%p") %> </p>
            <div class="bod">
            <span id="chatlog7" class="chatlog"></span>
            <span id="chatlog6" class="chatlog"></span>
            <span id="chatlog5" class="chatlog"></span>
            <span id="chatlog4" class="chatlog"></span>
            <span id="chatlog3" class="chatlog"></span>
            <span id="chatlog2" class="chatlog"></span>
            <span id="chatlog1" class="chatlog">Hello</span>
          </div>
            <footer class="chatbox-popup__footer">
              <!-- <aside style="flex:1;color:#888;text-align:center;">
                <i class="fa fa-camera" aria-hidden="true"></i>
              </aside> -->
              <aside style="flex:10">
                <input type="text" name="chat" id="chatbox"  placeholder="Reply to TWbot" onfocus="placeHolder()">
              </aside>
               <aside style="flex:1;color:#888;text-align:center;">
                <button class="message-submit">
                  <img src="/assets/img/send.png" alt="Image" />
                </button>
              </aside>

            </footer>
          </div>
        </div>
      </section>
    </div> 
    </div>
    <%#= render 'layouts/footer'  unless @disable_nav %>
    <% if params[:controller] == "admins" && params[:action] == "index" ||params[:controller] == "admins" && params[:action] == "show"||params[:controller] == "registrations" && params[:action] == "edit"||params[:controller] == "registrations" && params[:action] == "update"%>
    <% else %>
      <%= render 'layouts/footer'  %>
    <% end %>
    <script>
